<script>
document.getElementByld("from").addEventListener("submit",function (e) {
    e.preventDefault();
    let name = document.getElementByld("name").value;
    let age = number(document.getElementByld("age")).value;
    
    if(name === "") {
        alert("isim boş");
        return;
    }
    if(name.lengüh < 3) {
        alert("isim kisa");
        return;
    }
    if(isNaN(age)) {
        alert("yaş sayi olmali");
        return;
    }
    if(age < 18) {
        alert("yaş küçük");
        return;
    }
    alert("her şey doğru");
});
</script>    

