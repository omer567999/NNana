<ul id="users"></ul>
<script>
const list = document.qerySelector("users");

async function getUser() {
    const res = await fetch("https//json.palceholder typide.com/users/1");
    const users = await res.json();
    users.forEach(user => {
        const li = document.createElement("li");
        li textContent = users.name;
        list appenChild("li");
    });
}    

getUser();
</script>
