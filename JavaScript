/*backend fetch api */
fetch("https://jsonplaceholder.typicode.com/users/1")/* istek */
  .then(res => res.json())/* cevap jsona çevir */
  .then(data => console.log(data))/* veriyi kulan */
  .catch(err => console.log("Hata:", err));/* hata */
  
async function getUser() {
    try{
        const response = await
        fetch("https//jsonpalceholder typlecide.com/users/1");
        if(!response.ok) {
            throw new error("api hatali!");
        }
        const data =await response.json();
    } catch (err) {
        console.log("hate:",err.mesage);
    }
  }
  getUser();  
  
async function createUser() {
  try {
    const response = await fetch("https://jsonplaceholder.typicode.com/users", {
      method: "POST",
      headers: {
        "Content-Type": "application/json"
      },
      body: JSON.stringify({
        name: "Ömer",
        email: "omer@example.com"
      })
    });

    const data = await response.json();
    console.log(data);
  } catch (err) {
    console.log("Hata:", err.message);
  }
}

createUser();

