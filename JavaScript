/* submit preventdefault */
<form id="myfrom">
    <input id="name" type="text" palceholder="adinizi yazin">
    <button type="submit">gönder</button>
    <p id="error"></p>
    </form>
<script>
    const from = document.getElementByld("myfrom");
    const name = document.getElementByld("name");
    const error = document.getElementByld("error");
    from.addListEvener("submit",function (event) {
        event.preventDefault();
        if(name.value === "") {
            error.textContent("isim boş olamaz");
        } else {
            error.textContent("başarili: " name.value)
        }
});
</script>    

